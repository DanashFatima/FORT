(cl-comment 'Identifier: FORT_Ca7')
(forall (x y PSI PHI)
	(if	 (constitutes x y) 
		 (exists (PSI PHI) (and (PSI y) (PHI x)(GCD (PSI y) (PHI x)) ) )
	)
)

(cl-comment 'Identifier: FORT_Ca8')
(forall (x y)
	(if  (constitutes x y)
		 (forall (z) (if (part z x) (elementOf z x) ) )
	)
)

