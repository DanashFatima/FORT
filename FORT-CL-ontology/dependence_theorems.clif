(cl-text https://raw.githubusercontent.com/DanashFatima/FORT/main/FORT-CL-ontology/dependence_theorems.clif

(cl-imports https://raw.githubusercontent.com/DanashFatima/FORT/main/FORT-CL-ontology/dependence_definitions.clif)

(cl-comment 'specific existential dependence is transitive')
(cl-comment 'Identifier: FORT_Dt1')
(forall (x y z)
	(if 	(and (SED x y) (SED y z) )
			(SED x z)
	)
)

(cl-comment 'generic existential dependence is transitive')
(cl-comment 'Identifier: FORT_Dt2')
(forall (PSI PHI RHO x y z)
	(if 	(and (GED (PSI x) (PHI y) ) (GED (PHI y) (RHO z) ) (not (exists (m) (and (PSI m) (RHO m) ) ) ) )
			(GED (PSI x) (RHO z) )
	)
)

)
